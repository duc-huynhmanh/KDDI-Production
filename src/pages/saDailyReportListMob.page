<body class="page-schedule">
  
  <c:saMainTopComp typeOfScreen="{!typeOfScreen}" />

  <div class="protect-screen hide"></div>
  
  <apex:inputHidden id="messageInfo" html-class="messageInfo" value="{!messageInfo}"/>
  
  <!--value="{!messageInfo}"-->
  
  <apex:actionFunction name="jsRefreshInfoMessage" action="{!dummyAction}" rerender="report-detail-form,listComments,messageInfo"/>
  
  <!-- template-popup -->
  <div class="modal-dialog hide"><p>ダイアログメッセージ</p><div class="btn-cancel"><a href="#">キャンセル</a></div><div class="btn-ok"><a href="#">OK</a></div></div>
 
  <div class="document-container">
    <header class="document-header">
      <div class="btn-menu"><a href="#">ナビゲーション</a></div>
    </header>

    <apex:inputHidden html-class="currentInquiryReply" value="{!currentInquiryReplyId}" />

    <apex:actionFunction name="jsShowInquiry"
                         action="{!showCurrentInquiryDetail}"
                         rerender="pnlCurrentInquiry"
                         oncomplete="setView_DRL_02();"/>

    <div class="document-contents">

      <apex:actionRegion renderRegionOnly="false">
        
        <!-- #View DRL-01(typ.) -->
        <!--<section class="search-list hide admin">-->
        <section class="search-list hide">
          <header class="search-list-header user">
            <div>
              <div class="search-text"><input type="text" value="" placeholder="検索" /></div>
              <a href="#" class="btn-search ui-color-1">検索</a>
            </div>
          </header>
          <header class="search-list-header admin">
            <div>

              <div class="search-text">
                <apex:selectList value="{!scopeEvents}"
                                 size="1">
                  <apex:selectOption itemValue="private"
                                     itemLabel="自分の活動"
                                     html-class="private"/>
                  <apex:selectOption itemValue="organization"
                                     itemLabel="自組織の活動"
                                     html-class="organization"/>
                  <apex:actionSupport event="onchange"
                                      action="{!getDailyReportSearch}"
                                      reRender="pnlListEvents, pnlFiltersRoles"
                                      oncomplete="setLogic_SearchListEvents();"/>
                </apex:selectList>

              </div>

              <a href="#" class="btn-search-conditions ui-color-1 off">検索条件表示</a>



              <div class="search-conditions">


                <div class="search-text">
                  <apex:inputText value="{!filterKeyword}"
                                  html-placeholder="キーワード検索（３つまで）"/>
                  
                </div>


                <apex:outputPanel id="pnlFiltersRoles">
                  <apex:outputPanel rendered="{!displayFilterRoles}">

                    <div class="field ">
                      <label class="field-label"
                             style="padding-bottom: 10px;">組織1</label>

                      <apex:selectList value="{!filterRole5Option}"
                                       styleclass="field-value search-field-product-name"
                                       size="1"
                                       html-data-field="Role5DeveloperName"
                                       style="margin-bottom: 4px;"
                                       onkeydown="if(event.keyCode==13){this.blur();}">
                        <apex:selectOptions value="{!role5Options}" />
                        <apex:actionSupport event="onchange"
                                            action="{!inputRole5Option}"
                                            reRender="sltListRole6, sltListRole7, sltListRole8" />
                      </apex:selectList>

                    </div>

                    <div class="field ">
                      <label class="field-label"
                             style="padding-bottom: 10px;">組織2</label>

                      <apex:selectList value="{!filterRole6Option}"
                                       id="sltListRole6"
                                       styleclass="field-value search-field-product-name"
                                       size="1"
                                       html-data-field="Role6DeveloperName"
                                       style="margin-bottom: 4px;"
                                       onkeydown="if(event.keyCode==13){this.blur();}">
                        <apex:selectOptions value="{!role6Options}" />
                        <apex:actionSupport event="onchange"
                                            action="{!inputRole6Option}"
                                            reRender="sltListRole7, sltListRole8" />
                      </apex:selectList>

                    </div>

                    <div class="field ">
                      <label class="field-label"
                             style="padding-bottom: 10px;">組織3</label>

                      <apex:selectList value="{!filterRole7Option}"
                                       id="sltListRole7"
                                       styleclass="field-value search-field-product-name"
                                       size="1"
                                       html-data-field="Role7DeveloperName"
                                       style="margin-bottom: 4px;"
                                       onkeydown="if(event.keyCode==13){this.blur();}">
                        <apex:selectOptions value="{!role7Options}" />
                      </apex:selectList>

                    </div>

                  </apex:outputPanel>
                </apex:outputPanel>



                <div class="date">
                  <div style="display: inline-block">
                    <apex:inputText value="{!filterDateStart}"
                                    html-placeholder="開始日"
                                    html-readonly="true"
                                    styleclass="begin-date datepicker" />
                    <a href="#"
                       class="btn-reset ui-color-3"
                       onclick="$j('.begin-date').val('');">解除</a>
                  </div>

                  <span class="divider"> - </span>
                  <div style="display: inline-block">
                    <apex:inputText value="{!filterDateEnd}"
                                    html-placeholder="終了日"
                                    html-readonly="true"
                                    styleclass="end-date datepicker" />
                    <a href="#"
                       class="btn-reset ui-color-3"
                       onclick="$j('.end-date').val('');">解除</a>
                  </div>
                </div>

                <apex:commandLink value="検索"
                                  styleclass="btn-search ui-color-1"
                                  action="{!getDailyReportSearch}"
                                  reRender="pnlListEvents"
                                  oncomplete="setLogic_SearchListEvents();$j('.search-conditions').hide();"/>

              </div>




            </div>
          </header>

          <div class="search-result">

            <apex:outputPanel id="pnlListEvents">
              <ul>

                <apex:repeat value="{!lsClsEvents}" var="varEvent" >
                  <li class="result-date-item">
                    <h3 class="result-date" style="{!IF(varEvent.isRupture, '', 'display: none;')}"><time>{!varEvent.dateStr}</time></h3>
                    <ul>
                      <li class="report-item send" inqId="{!varEvent.evt.Id}">
                        <apex:outputPanel >
                        <h4 class="title">{!varEvent.evtTitle}</h4>
                        <p class="subject">{!varEvent.description}</p>
                        </apex:outputPanel>
                      </li>
                    </ul>
                  </li>
                </apex:repeat>
             
              </ul>
            </apex:outputPanel>            

          </div>
        </section>
      </apex:actionRegion>


      <!-- #View DRL-02 -->
      <section class="report-entry-form daily-report-detail hide">
        <div>
          <header>
<!--
            <div class="btn-save ui-color-1"><a href="#">保存</a></div>
-->
            <div class="title"><h2>日報詳細</h2></div>
            <a href="#" class="btn-close">閉じる</a>
          </header>
          <div class="report-entry-form-body">
