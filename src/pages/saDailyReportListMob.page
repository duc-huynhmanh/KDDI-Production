<apex:page controller="saDailyReportListCtrl" showHeader="false" sidebar="false" standardStylesheets="false" applyHtmlTag="false" applyBodyTag="false" cache="false" action="{!redirectIfNecessaryMobile}" docType="html-5.0">

<apex:form id="allForm">

<html lang="ja">
<head>
  <meta charset="utf-8"/>
  <title>auポータル</title>
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no"/>

  <apex:stylesheet value="{!URLFOR($Resource.auResPortalSA,'css/sanitize.css')}"/>
  <apex:stylesheet value="{!URLFOR($Resource.auResPortalSA,'css_sp/common.css')}"/>
  <apex:stylesheet value="{!URLFOR($Resource.auResSchedule,'/css_sp/style-search.css')}"/>
  <apex:stylesheet value="{!URLFOR($Resource.auResPortalSA,'css_sp/iphone.css')}"/>
  <apex:stylesheet value="{!URLFOR($Resource.auResPortalSA,'css/flatpickr.min.css')}"/>
  <apex:stylesheet value="{!URLFOR($Resource.auResPortalSA,'css_sp/style-calender.css')}"/>

  <!-- libraries -->
  <apex:includescript value="{!URLFOR($Resource.auResSchedule,'js/jquery-3.2.1.min.js')}"/> 
  <apex:includescript value="{!URLFOR($Resource.auResSchedule,'truncate/trunk8.js')}"/> 

  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/flatpickr/dist/flatpickr.min.css" />
  <script src="https://cdn.jsdelivr.net/npm/flatpickr"></script>
  <script src="https://npmcdn.com/flatpickr/dist/l10n/ja.js"></script>

  <!-- scripts -->
  <apex:includescript value="{!URLFOR($Resource.auResSchedule,'js_sp/jquery_custom-dialog.js')}"/> 
  <apex:includescript value="{!URLFOR($Resource.auResSchedule,'js_sp/script_check_ua.js')}"/> 
  <apex:includescript value="{!URLFOR($Resource.auResSchedule,'js_sp/script_fitting.js')}"/> 
  <apex:includescript value="{!URLFOR($Resource.auResSchedule,'js_sp/script_search.js')}"/> 
  <apex:includescript value="{!URLFOR($Resource.auResSchedule,'js_sp/script_sp.js')}"/> 
  

  <script>

    var $j = jQuery.noConflict();

    function jsTruncate() {
      $j('.truncate').trunk8();
    }

    function jsChangeRadioCheckedDefault() {
      $j('.isReplyCheckedDefault').prop('checked', true);
      $j('.isReplyDisabledDefault').attr('disabled', true);
    }

	function jsDisplayMessage() {
        if ($j('.messageInfo').val().length > 0) {
          	alert($j('.messageInfo').val());
          	$j('.messageInfo').val("");
        }
    }

  </script>

  <style type="text/css">
/*
    .search-result {
      padding-bottom: 0;
    }
*/
    ul li, ol li {
      margin-left: 0;
    }
    h1, h2, h3, h4, h5, h6 {
      display: inherit;
    }

    body a:hover {
      text-decoration: none;
    }

    .report-entry-form-body {
      padding-bottom: 0;
    }

    .event-list-item-title, .event-list-item-title-note {
      white-space: nowrap;
    }

    input:disabled, textarea:disabled {
      opacity: 1;
    }


    .isEvent .description {
      display: none;
    }

    .isNotEvent .event-list {
      display: none;
    }

    .search-conditions .begin-date, .search-conditions .end-date {
      width: 199px;
      font-size: 1.4rem;
    }

    .item-top > a {
      font-size: 1.8rem !important;
    }

input[type="date"] {
    vertical-align: top;
}

    .related-files {
      margin: 10px 0;
      margin-top: 10px;
      margin-right: 0px;
      margin-bottom: 10px;
      margin-left: 0px;
      padding: 0 20px;
      position: relative;
    }

    .button--delete-list {
/*
      width: 5.75rem;
      height: 2.5rem;
*/
      background-color: #000;
      color: #fff;
/*
      font-size: .833rem;
      padding: .8rem;
      line-height: 1;
*/
      background-image: none;
      border-width: 0;
      font-size: 2rem;
      line-height: 2;

    }


    .export-csv {
      display: inline-block;
      width: 230px;
      height: 60px;
      box-sizing: border-box;
      border-radius: 3px;
      text-align: center;
      font-size: 15px;
      font-size: 1.5rem;
      line-height: 60px;
      color: #ffffff;
      background-color: #ed5500;
    }

    .export-csv:hover {
      color: #ffffff;
