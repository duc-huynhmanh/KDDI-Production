      width: auto !important;
      font-size: 2.5rem !important;
    }

    .search-list-header.admin select {
      font-size: 3.5rem;
      color: black;
    }
    .search-text input {
      font-size: 3.5rem !important;
      color: black;
    }
    .search-conditions .begin-date,
