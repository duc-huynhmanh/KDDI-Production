                <caption>カレンダー</caption>
                <thead>
                  <tr>
                    <th>日</th>
                    <th>月</th>
                    <th>火</th>
                    <th>水</th>
                    <th>木</th>
                    <th>金</th>
                    <th>土</th>
                  </tr>
                </thead>
                <tbody>

                  <apex:repeat value="{!lsDaysOfMonth}" var="varWeek" >
                    <!-- week x -->
                    <tr>
                      <apex:repeat value="{!varWeek}" var="varDay">
                        <td class="calender-item {!IF(varDay.isBlank, 'blank', '')} {!IF(varDay.isToday, 'today', '')} {!IF(varDay.isSaved, 'saved', '')} {!IF(varDay.isSent, 'send', '')} {!IF(varDay.isSunday, 'sunday', '')} {!IF(varDay.isSaturday, 'saturday', '')}" BusinessLocation="{!varDay.idBusinessLocation}"><a href="#"><time>{!IF(varDay.isBlank, '', varDay.dayDate)}</time></a></td>
