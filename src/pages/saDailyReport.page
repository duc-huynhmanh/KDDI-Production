  <script>

    function setView_04_01_01() {

      $j(".document-header").show();
      $j('.calendar-section-top').show();
      $j('.calendar-section-list').hide();
      $j('.calendar-section-detail').hide();

      setView_04_01_01_logic();
    }

    function setView_04_01_01_logic() {

      $j(".calender-item").off("click");
      $j(".calender-item").on("click", onClickCalenderItem);

    }

    function setView_04_01_02() {

      $j(".document-header").hide();
      $j('.calendar-section-top').hide();
      $j('.calendar-section-list').show();
      $j('.calendar-section-detail').hide();

      setView_04_01_02_logic();

    }

    function setView_04_01_02_logic() {

      $j(".btn-back").off("click");
      $j(".btn-back").on("click", function (e) {
        setView_04_01_01();
      });

      $j(".btn-create").off("click");
      $j(".btn-create").on("click", function (e) {

        /* Get the checked Business Location */
        $j('.idBusinessLocation').val($j('input:radio[name=radiobuttonsBL]:checked').val());
        jsShowInquiry();

      });
      
    }

    function setView_04_01_03(activateButtons) {

      $j(".document-header").hide();
